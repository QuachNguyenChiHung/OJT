# AWS Configuration
AWS_REGION=ap-southeast-1
AWS_ACCOUNT_ID=your-account-id

# Lambda Configuration
LAMBDA_RUNTIME=nodejs20.x
LAMBDA_TIMEOUT=30
LAMBDA_MEMORY=512

# S3 Bucket for Lambda Code (created by CDK)
LAMBDA_CODE_BUCKET=ojt-lambda-code-bucket

# Database Configuration (from RDS)
DB_HOST=your-rds-endpoint
DB_NAME=OJT_Database
DB_SECRET_ARN=arn:aws:secretsmanager:region:account:secret:name

# JWT Configuration
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRES_IN=7d

# S3 Images Bucket (from CDK)
S3_IMAGES_BUCKET=ojt-images-bucket

# Bedrock Agent Configuration
BEDROCK_AGENT_ID=YASFITHPLV
BEDROCK_AGENT_ALIAS_ID=OP2BHIXLGL

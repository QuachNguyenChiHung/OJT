{
  "name": "ojt-deploy",
  "version": "1.0.0",
  "description": "Deployment scripts for OJT E-commerce Lambda functions",
  "scripts": {
    "deploy": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:skip-layer": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -SkipLayer",
    "deploy:layer": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -LayerOnly",
    "build": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -BuildOnly",
    "deploy:auth": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Module auth",
    "deploy:products": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Module products",
    "deploy:cart": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Module cart",
    "deploy:orders": "powershell -ExecutionPolicy Bypass -File deploy.ps1 -Module orders",
    "clean": "node -e \"require('fs').rmSync('./build', {recursive:true,force:true})\""
  },
  "keywords": ["aws", "lambda", "deploy"],
  "license": "ISC",
  "devDependencies": {
    "dotenv": "^16.3.1"
  }
}
